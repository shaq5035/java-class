package com.class9;

public class Columns {
// i want to print 
//***** 4 times
	public static void main(String[] args) {
		for (int i = 1; i <= 4; i++) {
			for (int k = 1; k <= 5; k++) {
				System.out.print("*");
			}
			System.out.println();
		}
		for(int b=4;b>=1;b--) {
			for(int c=1;b<=5;c++) {
				System.out.print(b);
			}
			System.out.println();
		}
	}

}
