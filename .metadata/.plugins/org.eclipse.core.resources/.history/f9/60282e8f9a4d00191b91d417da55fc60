package com.class11;

public class RetrivingAllValues {
	public static void main(String[] args) {
		
		int[][]num= {
				
				{1,2,3,4},
				{2,4,6,8,},
				{3,6,9,12},
		};
//		int rows=num.length;
//		System.out.println(rows);
//		
//		int col=num[0].length;
//		System.out.println(col);
		
		for(int row=0; row<num[row].length;row++) {
			
			for(int col1=0; col1<num[row].length;col1++) {
				System.out.print(num[row][col1]+" ");
			}
			System.out.println();
		}
	}

}
