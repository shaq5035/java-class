package com.class6;

import java.util.Scanner;

public class Calculator {

	public static void main(String[] args) {
		int num1,num2, sum = 0;
		String operator = "";
		
		Scanner input=new Scanner(System.in);
		System.out.println("Please enter 2 values");
		num1=input.nextInt();
		num2=input.nextInt();
		
		switch (operator) {
		
		case ("+"):
			sum= (num1+num2);
			break;
		case("-"):
			sum=(num1-num2);
			break;
		case ("*"):
			sum=(num1*num2);
			break;
		case("/"):
			sum=(num1/num2);
			break;
		default:
			operator="invalid";
		}
		System.out.println("Your sum is "+sum);
	}
	

}
