package com.class11;

public class Interger {

	public static void main(String[] args) {

		int sum=0;
		int []num= {1,2,3,4,5,6,7,8};
		for(int i=0; i<num.length;i++) {
			sum=sum+i;
			System.out.print(sum);
			System.out.println();
		}
		for(int numbers:num) {
			sum=sum+numbers;
			System.out.print(sum);
		}
	}

}
